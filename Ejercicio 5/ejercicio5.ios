config t

hostname RF

service password-encryption
no ip domain-lookup

enable password class

!Excluir IP DHCP
ip dhcp excluded-address 192.168.2.1
ip dhcp excluded-address 192.168.2.2

!DHCP
ip dhcp pool RFG0/1
network 192.168.2.0 255.255.255.0
dns-server 142.251.32.46
!Direccion de gateway del Router
default-router 192.168.2.1

line con 0
password cisco
logging synchronous
login

line vty 0 4
password cisco
login

banner motd # Solo acceso autorizado #

interface GigabitEthernet0/0
description Link to LAN 2
ip address 192.168.2.1 255.255.255.0
no shut

interface Serial0/0/0
description Link to R1
ip address 192.168.12.2 255.255.255.252
no shut

interface Serial0/0/1
description Link to R3
ip address 192.168.23.1 255.255.255.252
clock rate 128000
no shut

interface Serial0/1/0
description Link to ISP
ip address 200.6.10.194 255.255.255.224
no shut

! Ruta por default hacia el ISP
ip route 0.0.0.0 0.0.0.0 s0/1/0

router ospf 1
network 192.168.2.0 0.0.0.255 area 0
network 192.168.12.0 0.0.0.3 area 0
network 192.168.23.0 0.0.0.3 area 0
passive-interface g0/0
passive-interface s0/1/0
default-information originate

