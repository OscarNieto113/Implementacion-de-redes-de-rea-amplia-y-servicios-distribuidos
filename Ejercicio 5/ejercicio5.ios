! Rutas est√°ticas hacia las redes de la empresa
ip route 192.168.1.0 255.255.255.0 s0/0/0
ip route 192.168.2.0 255.255.255.0 s0/0/0
ip route 192.168.3.0 255.255.255.0 s0/0/0

---------------------------------------------
ROUTER R1
---------------------------------------------
en
config t

hostname R1

service password-encryption
no ip domain-lookup

enable password class

!Excluir IP DHCP
ip dhcp excluded-address 192.168.1.1

!DHCP
ip dhcp pool R1G0/1
network 192.168.1.0 255.255.255.0
dns-server 142.251.32.46
!Direccion de gateway del Router
default-router 192.168.1.1

line con 0
password cisco
logging synchronous
login

line vty 0 4
password cisco
login

banner motd # Solo acceso autorizado #

interface GigabitEthernet0/0
description Link to LAN 1
ip address 192.168.1.1 255.255.255.0
no shut

interface Serial0/0/0
description Link to R2
ip address 192.168.12.1 255.255.255.252
clock rate 128000
no shut


router ospf 1
network 192.168.1.0 0.0.0.255 area 0
network 192.168.12.0 0.0.0.3 area 0
passive-interface g0/0

